startshape fgsfdser

rule fgsfds {
	SQUARE{sat 1 b .9 hue 120 x .5}
	SQUARE{sat 1 b .9 skew 0 45 y -.5 x -.5}
	SQUARE{sat 1 b .9 hue 240 skew 45 0 y -1}
	SQUARE{sat 1 b .9 hue 240 skew 45 0}
	SQUARE{sat 1 b .9 skew 0 45 y -.5 x .5}
	SQUARE{sat 1 b .9 hue 120 y -1 x -.5}
}
rule fgsfdser {
	fgsfds{a -.7}
	fgsfdser{x 1}
}
rule fgsfdser {
	fgsfds{a -.7}
	fgsfdser{x -1}
}
rule fgsfdser {
	fgsfds{a -.7}
	fgsfdser{y 1}
}
rule fgsfdser {
	fgsfds{a -.7}
	fgsfdser{y -1}
}
